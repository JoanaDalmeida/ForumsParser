<?xml version="1.0" encoding="ISO-8859-1"?>
<parameters>
<!-- A page wcich contents post, it is used to analyze posts container characteristic -->
<samplePageUrl></samplePageUrl>

<!-- A post sample, it is used to get the post container tag and class -->
<postSample>
	<postDate></postDate>
	<postAurhor></postAurhor>
	<postMessage></postMessage>
	<isPostIdExist></isPostIdExist>
</postSample>



<!--true or false, true if you want to use parameters File to init posts params else put false-->
<useParametersFile></useParametersFile>

<!--Posts Parameters File as input-->
<postsParamsInput></postsParamsInput>



<!--true or false, true if you want to parse URLS in a giving File-->
<useUrlsInFile></useUrlsInFile>

<!--path to File that contains URLs to parse-->
<urlsFileName></urlsFileName>



<!--Delimiter of the CSV file in which the program saves all posts-->
<DelimiterOfPostsFile></DelimiterOfPostsFile>

<!--path of the CSV file in which the program saves all posts-->
<postsFileName></postsFileName>

<!--path of the CSV file in which the program saves all posts-->
<discussionsFileName></discussionsFileName>

<!--path of the file in which the program saves posts's Parameters-->
<postsParamsFileName></postsParamsFileName>

<!--path of the file in which the program saves failed Urls-->
<failedUrlsFileName></failedUrlsFileName>

<!--path of the file in which the program saves URLs that it parses-->
<succedUrlsFileName></succedUrlsFileName>



<!--The url of the forum to parse -->
<forumToParseURL></forumToParseURL>

<!--keywords that the page url must contain, you can add many keywords-->
<keyWordsInURL></keyWordsInURL>

<!--keywords that the page url doesn't contain, you can add many exclusion keywords-->
<exclusionKeyWords></exclusionKeyWords>


<!-- Second to crawls the website-->
<PaginationMethod>
<!--true or false, true if you want to use pagination to parse website-->
<usePagination>true</usePagination>

<!-- the first page of discussion Lists-->
<dicussionsListFirstPage></dicussionsListFirstPage>
<!-- Specify the second page of discussion Lists Page -->
<dicussionsListSecondPage></dicussionsListSecondPage>

<!-- the first page of posts Lists of discussion-->
<postsListPageStartWith></postsListPageStartWith>
<!-- Specify what change from one posts Lists Page to another-->
<postsListPattern></postsListPattern>

</PaginationMethod>


</parameters>